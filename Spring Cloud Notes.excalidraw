      "fontSize": 16,
      "fontFamily": 5,
      "text": "If the Gateway Handler Mapping determines that a request \nmatches a route it is sent to the Gateway Web Handler",
      "textAlign": "left",
      "verticalAlign": "top",
      "containerId": null,
      "originalText": "If the Gateway Handler Mapping determines that a request matches a route it is sent to the Gateway Web Handler",
      "autoResize": false,
